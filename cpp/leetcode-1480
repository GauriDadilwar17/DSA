//running sum of 1d array
class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        vector<int>result;
        for(int i=0; i<nums.size(); i++){
            int runningSum=0;
            for(int j=0; j<=i; j++){
             runningSum += nums[j];          
           }
            result.push_back(runningSum);
        }
           
            return result;
    }
};
